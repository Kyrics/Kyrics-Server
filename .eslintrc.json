{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {        
        "arrow-body-style": "off",
        "comma-dangle": "off",
        "object-curly-newline": "off",
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "linebreak-style": "off",
        "import/prefer-default-export": "off"
    }
}
